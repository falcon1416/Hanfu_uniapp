<form report-submit data-event-opts="{{[['submit',[['formSubmit',['$event']]]]]}}" bindsubmit="__e"><view class="cu-form-group margin-top"><view class="title">logo</view><view data-event-opts="{{[['tap',[['handleChooseImage',['$event']]]]]}}" class="solids" bindtap="__e"><text class="cuIcon-cameraadd"></text></view></view><view class="cu-form-group margin-top"><view class="title">店名</view><input placeholder="请输入店名" data-event-opts="{{[['input',[['__set_model',['$0','name','$event',[]],['info']]]]]}}" value="{{info.name}}" bindinput="__e"/></view><view class="cu-form-group margin-top"><view class="title">分类</view><checkbox-group data-event-opts="{{[['change',[['onCheckChange',['$event']]]]]}}" bindchange="__e"><block wx:for="{{typeList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><label><checkbox class="{{[item.isSelected?'checked':'']}}" checked="{{item.isSelected?true:false}}" value="{{item.value}}"></checkbox><text>{{item.label}}</text></label></block></checkbox-group></view><view class="cu-form-group margin-top"><view class="title">标签</view><input placeholder="多个标签,空格区分" data-event-opts="{{[['input',[['__set_model',['$0','tag','$event',[]],['info']]]]]}}" value="{{info.tag}}" bindinput="__e"/></view><view class="cu-form-group align-start margin-top"><view class="title">复制码</view><textarea maxlength="-1" placeholder="淘宝分享的复制码" data-event-opts="{{[['input',[['__set_model',['$0','share','$event',[]],['info']]]]]}}" value="{{info.share}}" bindinput="__e"></textarea></view><view class="cu-form-group margin-top"><textarea maxlength="-1" placeholder="店铺介绍" data-event-opts="{{[['input',[['__set_model',['$0','intro','$event',[]],['info']]]]]}}" value="{{info.intro}}" bindinput="__e"></textarea></view><button class="margin-top" type="default" form-type="submit">保存</button></form>